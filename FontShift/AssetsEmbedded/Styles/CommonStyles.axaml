<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:System="clr-namespace:System;assembly=mscorlib"
        xmlns:Col="clr-namespace:System.Collections;assembly=mscorlib"
        xmlns:Data="using:FontShift.Application.Data.Xaml"
        xmlns:ucp="using:FontShift.Application.UserControls.Primitives">
  <Design.PreviewWith>
    <Border Width="1600" Height="1600" Background="Black">
      <StackPanel Orientation="Vertical" Spacing="20">
        <StackPanel Orientation="Horizontal" Spacing="20">
          <!-- Backgrounds -->
          <StackPanel Orientation="Vertical" Background="{StaticResource BackgroundDefault}" Spacing="6">
            <TextBlock Text="Backgrounds:" Foreground="{StaticResource FontColorDefault}" FontSize="{StaticResource FontSizeDefault}" Margin="0 0 0 32"/>
            <Grid Width="200" Height="40" Background="{StaticResource BackgroundDefault}">
              <TextBlock Text="BackgroundDefault" Foreground="{StaticResource FontColorDefault}" FontSize="{StaticResource FontSizeVeryLow}" />
            </Grid>
            <Grid Width="200" Height="40" Background="{StaticResource BackgroundLow}">
              <TextBlock Text="BackgroundLow" Foreground="{StaticResource FontColorDefault}" FontSize="{StaticResource FontSizeVeryLow}" />
            </Grid>
            <Grid Width="200" Height="40" Background="{StaticResource BackgroundMedium}">
              <TextBlock Text="BackgroundMedium" Foreground="{StaticResource FontColorDefault}" FontSize="{StaticResource FontSizeVeryLow}" />
            </Grid>
            <Grid Width="200" Height="40" Background="{StaticResource BackgroundHigh}">
              <TextBlock Text="BackgroundHigh" Foreground="{StaticResource FontColorDefault}" FontSize="{StaticResource FontSizeVeryLow}" />
            </Grid>
            <Grid Width="200" Height="40" Background="{StaticResource BackgroundVeryHigh}">
              <TextBlock Text="BackgroundVeryHigh" Foreground="{StaticResource FontColorDefault}" FontSize="{StaticResource FontSizeVeryLow}" />
            </Grid>
            <Grid Width="200" Height="40" Background="{StaticResource BackgroundHighest}">
              <TextBlock Text="BackgroundHighest" Foreground="{StaticResource FontColorHigh}" FontSize="{StaticResource FontSizeVeryLow}" />
            </Grid>
            <Grid Width="200" Height="40" Background="{StaticResource BackgroundHighlightedDefault}"  Margin="0 16 0 0">
              <TextBlock Text="BackgroundHighlightedDefault" Foreground="{StaticResource FontColorDefault}" FontSize="{StaticResource FontSizeVeryLow}" />
            </Grid>
            <Grid Width="200" Height="40" Background="{StaticResource BackgroundHighlightedVeryLow}">
              <TextBlock Text="BackgroundHighlightedVeryLow" Foreground="{StaticResource FontColorDefault}" FontSize="{StaticResource FontSizeVeryLow}" />
            </Grid>
            <Grid Width="200" Height="40" Background="{StaticResource BackgroundHighlightedLow}">
              <TextBlock Text="BackgroundHighlightedLow" Foreground="{StaticResource FontColorDefault}" FontSize="{StaticResource FontSizeVeryLow}" />
            </Grid>
            <Grid Width="200" Height="40" Background="{StaticResource BackgroundHighlightedHigh}">
              <TextBlock Text="BackgroundHighlightedHigh" Foreground="{StaticResource FontColorVeryDark}" FontSize="{StaticResource FontSizeVeryLow}" />
            </Grid>
            <Grid Width="200" Height="40" Background="{StaticResource BackgroundInfo}" Margin="0 16 0 0">
              <TextBlock Text="BackgroundInfo" Foreground="{StaticResource FontColorVeryHigh}" FontSize="{StaticResource FontSizeVeryLow}" />
            </Grid>
            <Grid Width="200" Height="40" Background="{StaticResource BackgroundWarning}">
              <TextBlock Text="BackgroundWarning" Foreground="{StaticResource FontColorVeryDark}" FontSize="{StaticResource FontSizeVeryLow}" />
            </Grid>
            <Grid Width="200" Height="40" Background="{StaticResource BackgroundError}">
              <TextBlock Text="BackgroundError" Foreground="{StaticResource FontColorVeryDark}" FontSize="{StaticResource FontSizeVeryLow}" />
            </Grid>
          </StackPanel>

          <!-- Font Colors -->
          <StackPanel Orientation="Vertical" Background="{StaticResource BackgroundDefault}" Spacing="8">
            <TextBlock Text="Font Colors:" Foreground="{StaticResource FontColorDefault}" FontSize="{StaticResource FontSizeDefault}" Margin="0 0 0 32"/>
            <TextBlock Text="Default" Foreground="{StaticResource FontColorDefault}" FontSize="{StaticResource FontSizeDefault}" />
            <TextBlock Text="VeryDark" Foreground="{StaticResource FontColorVeryDark}" FontSize="{StaticResource FontSizeDefault}" />
            <TextBlock Text="Dark" Foreground="{StaticResource FontColorDark}" FontSize="{StaticResource FontSizeDefault}" />
            <TextBlock Text="VeryLow" Foreground="{StaticResource FontColorVeryLow}" FontSize="{StaticResource FontSizeDefault}" />
            <TextBlock Text="Low" Foreground="{StaticResource FontColorLow}" FontSize="{StaticResource FontSizeDefault}" />
            <TextBlock Text="High" Foreground="{StaticResource FontColorHigh}" FontSize="{StaticResource FontSizeDefault}" />
            <TextBlock Text="VeryHigh" Foreground="{StaticResource FontColorVeryHigh}" FontSize="{StaticResource FontSizeDefault}" />
            <TextBlock Text="HighlightedDefault" Foreground="{StaticResource FontColorHighlightedDefault}" FontSize="{StaticResource FontSizeDefault}" Margin="0 16 0 0" />
            <TextBlock Text="HighlightedLow" Foreground="{StaticResource FontColorHighlightedLow}" FontSize="{StaticResource FontSizeDefault}" />
            <TextBlock Text="HighlightedHigh" Foreground="{StaticResource FontColorHighlightedHigh}" FontSize="{StaticResource FontSizeDefault}" />
            <TextBlock Text="Info" Foreground="{StaticResource FontColorInfo}" FontSize="{StaticResource FontSizeDefault}" Margin="0 16 0 0" />
            <TextBlock Text="Warning" Foreground="{StaticResource FontColorWarning}" FontSize="{StaticResource FontSizeDefault}" />
            <TextBlock Text="Error" Foreground="{StaticResource FontColorError}" FontSize="{StaticResource FontSizeDefault}" />
          </StackPanel>

          <!-- Font Sizes -->
          <StackPanel Orientation="Vertical" Background="{StaticResource BackgroundDefault}" Spacing="8">
            <TextBlock Text="Font Sizes:" Foreground="{StaticResource FontColorDefault}" FontSize="{StaticResource FontSizeDefault}" Margin="0 0 0 32"/>
            <TextBlock Text="Default" Foreground="{StaticResource FontColorDefault}" FontSize="{StaticResource FontSizeDefault}" />
            <TextBlock Text="VeryLow" Foreground="{StaticResource FontColorDefault}" FontSize="{StaticResource FontSizeVeryLow}" />
            <TextBlock Text="Low" Foreground="{StaticResource FontColorDefault}" FontSize="{StaticResource FontSizeLow}" />
            <TextBlock Text="High" Foreground="{StaticResource FontColorDefault}" FontSize="{StaticResource FontSizeHigh}" />
            <TextBlock Text="VeryHigh" Foreground="{StaticResource FontColorDefault}" FontSize="{StaticResource FontSizeVeryHigh}" />
          </StackPanel>
        </StackPanel>

        <StackPanel Orientation="Horizontal" Spacing="20">

          <!-- Buttons -->
          <StackPanel Orientation="Vertical" Background="{StaticResource BackgroundDefault}">
            <TextBlock Text="Buttons:" Foreground="{StaticResource FontColorDefault}" FontSize="{StaticResource FontSizeDefault}" Margin="0 0 0 0"/>
            <StackPanel Orientation="Vertical" Spacing="16" Margin="16">
              <ucp:DefaultButton Text="Default Button" Width="300" Height="110" Icon="avares://FontShift/AssetsEmbedded/Images/icon-shutdown.png" />
              <ucp:DefaultButton Text="Default Button" />
              <ucp:DefaultButton Text="" Width="300" Height="110" Icon="avares://FontShift/AssetsEmbedded/Images/icon-shutdown.png" />
              <ucp:DefaultButton Text="Default Button" Width="300" Height="110" IsEnabled="False" IconScaleFactor="0.7" Icon="avares://FontShift/AssetsEmbedded/Images/icon-shutdown.png" />
            </StackPanel>
          </StackPanel>

          <StackPanel Orientation="Vertical" Background="{StaticResource BackgroundDefault}">
            <TextBlock Text="Buttons:" Foreground="{StaticResource FontColorDefault}" FontSize="{StaticResource FontSizeDefault}" Margin="0 0 0 0"/>
            <StackPanel Orientation="Vertical" Spacing="16" Margin="16">
              <ucp:DefaultToggleButton Text="Enabled Checked" Width="300" Height="110" IsChecked="True" Icon="avares://FontShift/AssetsEmbedded/Images/icon-shutdown.png" />
              <ucp:DefaultToggleButton Text="Enabled Unchecked" IsChecked="False" />
              <ucp:DefaultToggleButton Text="Disabled Checked" Width="300" Height="110" IsChecked="True" IsEnabled="False" Icon="avares://FontShift/AssetsEmbedded/Images/icon-shutdown.png" />
              <ucp:DefaultToggleButton Text="Disabled Unchecked" Width="300" Height="110" IsChecked="False" IsEnabled="False" IconScaleFactor="0.5" Icon="avares://FontShift/AssetsEmbedded/Images/icon-shutdown.png" />
            </StackPanel>
          </StackPanel>

          <!-- Icons -->
          <StackPanel Orientation="Vertical" Background="{StaticResource BackgroundDefault}">
            <TextBlock Text="Image Icons:" Foreground="{StaticResource FontColorDefault}" FontSize="{StaticResource FontSizeDefault}" Margin="0 0 0 0"/>
            <StackPanel Orientation="Vertical" Spacing="16" Margin="16">
              <Image Stretch="None" Source="{StaticResource DefaultInfoIcon}" />
              <Image Stretch="None" Source="{StaticResource DefaultWarningIcon}" />
              <Image Stretch="None" Source="{StaticResource DefaultErrorIcon}" />
            </StackPanel>
          </StackPanel>

          <StackPanel Orientation="Vertical" Background="{StaticResource BackgroundDefault}">
            <TextBlock Text="Path Icons:" Foreground="{StaticResource FontColorDefault}" FontSize="{StaticResource FontSizeDefault}" Margin="0 0 0 0"/>
            <StackPanel Orientation="Vertical" Spacing="16" Margin="16">
              <PathIcon Data="{StaticResource CameraPath}" Foreground="{StaticResource FontColorDefault}" />
              <PathIcon Data="{StaticResource MenuPath}" Foreground="{StaticResource FontColorDefault}" />
              <PathIcon Data="{StaticResource ClosePath}" Foreground="{StaticResource FontColorDefault}" />
              <PathIcon Data="{StaticResource ChevronRightPath}" Foreground="{StaticResource FontColorDefault}" />
              <PathIcon Data="{StaticResource ChevronDownPath}" Foreground="{StaticResource FontColorDefault}" />
              <PathIcon Data="{StaticResource SpacePath}" Foreground="{StaticResource FontColorDefault}" />
              <PathIcon Data="{StaticResource ArrowDownPath}" Foreground="{StaticResource FontColorDefault}" />
              <PathIcon Data="{StaticResource CheckmarkPath}" Foreground="{StaticResource FontColorDefault}" />
              <PathIcon Data="{StaticResource PersonPath}" Foreground="{StaticResource FontColorDefault}" />
              <PathIcon Data="{StaticResource InfoPath}" Foreground="{StaticResource FontColorDefault}" />
              <PathIcon Data="{StaticResource SettingsPath}" Foreground="{StaticResource FontColorDefault}" />
              <PathIcon Data="{StaticResource PausePath}" Foreground="{StaticResource FontColorDefault}" />
              <PathIcon Data="{StaticResource CheckmarkLockPath}" Foreground="{StaticResource FontColorDefault}" />
              <PathIcon Data="{StaticResource CheckboxCheckedPath}" Foreground="{StaticResource FontColorDefault}" />
              <PathIcon Data="{StaticResource CheckboxUncheckedPath}" Foreground="{StaticResource FontColorDefault}" />
              <PathIcon Data="{StaticResource LockPath}" Foreground="{StaticResource FontColorDefault}" />
            </StackPanel>
          </StackPanel>

        </StackPanel>

        <StackPanel Orientation="Horizontal" Spacing="20">

        </StackPanel>

      </StackPanel>
    </Border>
  </Design.PreviewWith>

  <Styles.Resources>

    <!-- ################################ -->
    <!-- ##### User Control Elements #### -->
    <!-- ################################ -->

